{
  "name": "Segment Event Tracker Custom",
  "manifest_version": 3,
  "version": "3.0",
  "description": "See when Segment and Dreamdata events are being tracked.",
  "permissions": [
  	"webRequest",
    "webRequestBlocking",
    "storage"
  ],
  "host_permissions": [
  	"<all_urls>",
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo.png"
  },
  "background": {
	  "service_worker": "background.js"
  },
  "devtools_page": "devtools_index.html"
}
